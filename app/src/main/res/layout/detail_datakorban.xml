<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/putih"
    android:paddingTop="20dp"
    android:paddingLeft="25dp"
    android:paddingRight="25dp"
    android:paddingBottom="20dp">

    <TextView
        android:id="@+id/TvDataKorban"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Data Korban"
        android:textColor="@color/hitam"
        android:textSize="16sp"
        fontPath="font/SFProDisplay-Bold.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:layout_marginTop="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nama"
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:id="@+id/TvNama"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Lorem.."
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:layout_marginTop="4dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Jenis Kelamin"
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:id="@+id/TvJenisKelamin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Lorem.."
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:layout_marginTop="4dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Disabilitas"
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:id="@+id/TvDisabilitas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Lorem.."
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:layout_marginTop="4dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Usia"
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:id="@+id/TvUsia"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Lorem.."
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:layout_marginTop="4dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pendidikan"
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:id="@+id/TvPendidikan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Lorem.."
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:layout_marginTop="4dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Bekerja"
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:id="@+id/TvBekerja"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Lorem.."
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:layout_marginTop="4dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Status Kawin"
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>

    <TextView
        android:id="@+id/TvStatusKawin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Lorem.."
        android:textSize="14sp"
        fontPath="font/SFProDisplay-Regular.ttf"
        tools:ignore="MissingPrefix"/>
</LinearLayout>